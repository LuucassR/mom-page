{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "./",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ESNext",
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": true,
    "exactOptionalPropertyTypes": false,
    "noImplicitAny": false
  },
  "include": ["src/**/*.ts", "lib/**/*.ts", "generated/**/*.ts"]
}